<?xml version="1.0" encoding="UTF-8"?>
<COMPONENT>
	<HEADER AUTHOR="$COMPONENT-AUTHOR$" COMPATIBILITY="ALL"
		DATA_AUTO_PROPAGATE="true" PLATEFORM="ALL" RELEASE_DATE="$RELEASE-DATE$"
		SCHEMA_AUTO_PROPAGATE="true" SERIAL="" SHORTNAME="$COMPONENT-NAME$"
		STARTABLE="true" STATUS="BETA" VERSION="$COMPONENT-VERSION$">
		<SIGNATURE />
	</HEADER>
	<FAMILIES>
		<FAMILY>Social Analytics/Twitter</FAMILY>
	</FAMILIES>
	<DOCUMENTATION>
		<URL>http://gabrielebaldassarre.com/talend/ttwitteroauth</URL>
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>
	<PARAMETERS>
		<PARAMETER NAME="CONSUMER_KEY" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="1" />
		<PARAMETER NAME="CONSUMER_SECRET" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="2" />
		<PARAMETER NAME="ACCESS_TOKEN" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="3" />
		<PARAMETER NAME="ACCESS_TOKEN_SECRET" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="4" />
		<PARAMETER NAME="CONNECTION_TYPE" FIELD="CLOSED_LIST"
			REQUIRED="true" NUM_ROW="5">
			<ITEMS DEFAULT="TWITTER">
				<ITEM NAME="TWITTER" VALUE="Twitter" />
				<ITEM NAME="TWITTERSTREAM" VALUE="TwitterStream" />
			</ITEMS>
		</PARAMETER>
	</PARAMETERS>
	<ADVANCED_PARAMETERS>
	</ADVANCED_PARAMETERS>
	<CODEGENERATION>
		<!-- Maven-managed import list DO NOT EDIT BELOW THIS LINE -->
		<!-- TOSIMPORTS -->
		<!-- You can edit again below this line -->
	</CODEGENERATION>
	<RETURNS>
		<RETURN NAME="USER_ID" TYPE="id_Long" AVAILABILITY="AFTER" />
		<RETURN NAME="SCREEN_NAME" TYPE="id_String" AVAILABILITY="AFTER" />
	</RETURNS>
</COMPONENT>